[
  {
    "comment": "サーバー起動開始",
    "regexp": [".+?]: (Starting minecraft server version .*)"],
    "webhook": [
      {
        "content": "$1"
      },
      {
        "embeds": [
          {
            "title": "Server has starting...",
            "color": 16429359
          }
        ]
      }
    ]
  },
  {
    "comment": "サーバー起動",
    "regexp": [".+?]: (Done \\(.+?s\\)! For help, type \"help\")"],
    "webhook": [
      {
        "content": "$1"
      },
      {
        "embeds": [
          {
            "title": "Server has started!",
            "color": 7258631
          }
        ]
      }
    ]
  },
  {
    "comment": "サーバー停止開始",
    "regexp": [".+?]: (Stopping server)"],
    "webhook": [
      {
        "content": "$1"
      },
      {
        "embeds": [
          {
            "title": "Server has stopping...",
            "color": 16429359
          }
        ]
      }
    ]
  },
  {
    "comment": "ワールド保存",
    "regexp": [".+?]: (ThreadedAnvilChunkStorage .* are saved)"],
    "webhook": [
      {
        "content": "$1"
      }
    ]
  },
  {
    "comment": "ワールド保存",
    "regexp": [".+?]: ThreadedAnvilChunkStorage: (All dimensions are saved)"],
    "webhook": [
      {
        "embeds": [
          {
            "title": "Server has saved!",
            "color": 7258631
          }
        ]
      }
    ]
  },
  {
    "comment": "Player Join",
    "regexp": [".+?]: (.+?) joined the game"],
    "webhook": [
      {
        "embeds": [
          {
            "author": {
              "icon_url": "https://minotar.net/helm/$1",
              "name": "$1 joined the game"
            },
            "color": 10070709
          }
        ]
      }
    ]
  },
  {
    "comment": "Left",
    "regexp": [".+?]: (.+?) left the game"],
    "webhook": [
      {
        "embeds": [
          {
            "author": {
              "icon_url": "https://minotar.net/helm/$1",
              "name": "$1 left the game"
            },
            "color": 10070709
          }
        ]
      }
    ]
  },
  {
    "comment": "Advancement",
    "regexp": [
      ".+?]: (.+?) (has made the advancement .*)",
      ".+?]: (.+?) (has completed the challenge .*)"
    ],
    "webhook": [
      {
        "embeds": [
          {
            "author": {
              "icon_url": "https://minotar.net/helm/$1",
              "name": "$1 $2"
            },
            "color": 10070709
          }
        ]
      }
    ]
  },
  {
    "comment": "Chat",
    "regexp": [".+?]: <(.+?)> (.*)"],
    "webhook": [
      {
        "username": "$1",
        "avatar_url": "https://minotar.net/helm/$1",
        "content": "$2"
      }
    ]
  },
  {
    "comment": "Death",
    "regexp": [
      ".+?]: (.+?) (was.*)",
      ".+?]: (.+?) (fell.*)",
      ".+?]: (.+?) (went.*)",
      ".+?]: (.+?) (walked into .*)",
      ".+?]: (.+?) (drowned.*)",
      ".+?]: (.+?) (died.*)",
      ".+?]: (.+?) (blew up)",
      ".+?]: (.+?) (hit the ground .*)",
      ".+?]: (.+?) (experienced kinetic energy.*)",
      ".+?]: (.+?) (fell from a high place)",
      ".+?]: (.+?) (froze to death)",
      ".+?]: (.+?) (discovered the floor was lava)",
      ".+?]: (.+?) (suffocated in a wall.*)",
      ".+?]: (.+?) (tried to swim in lava.*)",
      ".+?]: (.+?) (burned to death)",
      ".+?]: (.+?) (left the confines of this world.*)",
      ".+?]: (.+?) (didn't want to live in the same world as .*)",
      ".+?]: (.+?) (starved to death.*)",
      ".+?]: (.+?) (withered away.*)"
    ],
    "webhook": [
      {
        "embeds": [
          {
            "author": {
              "icon_url": "https://minotar.net/helm/$1",
              "name": "$1 $2"
            },
            "color": 10070709
          }
        ]
      }
    ]
  },
  {
    "comment": "Other",
    "regexp": [
      ".+?]: (.*default to:.*)",
      ".+?]: (.*now set to:.*)",
      ".+?]: (.*server operator.*)",
      ".+?]: (.*to the whitelist.*)",
      ".+?]: (.*from the whitelist.*)",
      ".+?]: (.*\\[Server\\] .*)",
      ".+?]: (.*\\[Rcon\\] .*)",
      ".+?]: (.+? lost connection: Server closed)",
      ".+?]: (.+? lost connection: Timed out)",
      ".+?]: (.+? lost connection: You logged in from another location)"
    ],
    "webhook": [
      {
        "content": "$1"
      }
    ]
  }
]
